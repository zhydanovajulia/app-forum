<table class="profile" summary="Profile information">

  <td class="feed" >
   <p>Theme name  </p>     
         <h1>    <%= @theme.content %> </h1>
     <% unless @posts.empty? %>

        <table class="posts" summary="User posts"  cellspacing="10">

        <%= render @posts %>

        </table>
        <%= will_paginate @posts %>
      <% end %>
   </td>
    
     <td class="sidebar round">
        <%= render 'shared/user_info' %>
     </td>
  </tr>
 <tr>
  
   <td class="main" >
         
        <h2 class="theme">Want create new post?</h2>
        <%= render 'shared/post_form' %>
    </td>
 </tr>
 

  
   
</table>
